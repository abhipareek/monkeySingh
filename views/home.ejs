    <section class="tf-section project">
        <div class="w_1280">
            <div class="row">
                <div class="col-md-12">
                    <div class="tf-title" data-aos="fade-up" data-aos-duration="800">
                        <h2 class="title">
                            Our Portfolio
                        </h2>
                        <div class="featured-countdown style2">
                            <p class="featured_title"> Whether it’s a new logo, marketing materials, advertisements, banner or website/app
                                design,
                                there's always a dedicated
                                designer for the job.</p>
                            <span class="slogan"></span>
                             
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="container_inner">
                        <div class="swiper-container slider-11" data-aos="fade-in" data-aos-duration="1000">
                            <div class="swiper-wrapper">
                                <% workArray.forEach( work=> { %>
                                <div class="swiper-slide">
                                    <div class="project-box-style6">
                                        <span class="boder"></span>
                                        <div class="img-box">
                                            <div class="image">
                                                <% if(work.category !="Motion-Video" ){ %>
                                                    <img class="rounded-md card-image" src="<%= work.image %>" alt="" />
                                                    <% }else{ %>
                                                    <video width="500" height="545" autoplay muted controls class="rounded-md card-image">
                                                      <source src="<%= work.image %>" type="video/mp4">
                                                    </video>
                                                    <% } %> 
                                                  
                                            </div>
                                             <div class="content-inner">
                                                            <h5 class="heading"> <%= work.category %></h5>
                                                    </div> 
                                          
                                        </div>
                                       
                                    </div>
                                </div>
                                <% }); %>

                             
                            </div>
               
                        </div>
                        <div class="next_slider-11 next_slider"><svg width="18" height="16" viewBox="0 0 18 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M1.5 8H16.5M16.5 8L9.75 1.25M16.5 8L9.75 14.75" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            </svg>
                            </div>
                        <div class="prev_slider-11 prev_slider"><svg width="18" height="16" viewBox="0 0 18 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M16.5 8H1.5M1.5 8L8.25 1.25M1.5 8L8.25 14.75" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                            </svg>
                            </div>
                    </div>
                   
                </div>
            </div>
        </div>
    </section>

